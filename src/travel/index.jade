extends ../_layouts/base

block config
  //- Correct base url pathing for this page
  - var baseUrl = '../'

block content
  main.travel
    section.travel__hero
      .travel__hero__background
        img(src="/images/travel/hero.jpg")
      .travel__hero__container
        .travel__hero__wrapper
          h2.travel__hero__title
            span viajá con nosotros
          p.travel__hero__text Elegí entre las múltiples opciones
    section.travel__content
      .container
        .row.nopadding
          .col-xs-12.col-md-6
            .travel__content__wrapper.travel__content__wrapper--col1(data-target="col1")
              .travel__content__background.travel__content__background--col1
              .travel__content__logo
                img(src="/images/travel/logo-1.png")
          .col-xs-12.col-md-6
            .travel__content__wrapper.travel__content__wrapper--col2(data-target="col2")
